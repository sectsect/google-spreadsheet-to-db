{
  "name": "sectsect/google-spreadsheet-to-db",
  "description": "Pulls Google Spreadsheet data via Googleâ€™s API and saves it in your wordpress database.",
  "keywords": [
    "github",
    "wordpress",
    "plugin",
    "google",
    "spreadsheet",
    "array"
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.5",
    "google/apiclient": "^2.16"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^9.5",
    "yoast/phpunit-polyfills": "^2.0",
    "squizlabs/php_codesniffer": "^3.7",
    "wp-coding-standards/wpcs": "^3.0",
    "phpstan/phpstan": "^1.10",
    "szepeviktor/phpstan-wordpress": "^1.3",
    "phpstan/extension-installer": "^1.3"
  },
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "SECT WEB INTERACTIVE",
      "homepage": "https://www.ilovesect.com/"
    }
  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse --memory-limit=2048M"
  }
}
